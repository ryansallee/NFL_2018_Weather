<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="413.0" prefWidth="333.0" spacing="20.0" stylesheets="@css/buttonstyles.css" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.codelouisville.Controllers.PrimaryController">
   <children>
      <Label text="Welcome to NFL Weather" />
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="20.0">
         <children>
            <Button fx:id="primaryButton" onAction="#switchToScatter" text="View Scatter Chart of Scoring and Temperature" />
            <Button mnemonicParsing="false" onAction="#switchToTemperatureBar" text="View Bar Chart of Scoring and Temperature" />
            <Button mnemonicParsing="false" onAction="#switchToConditonBar" text="View Bar Chart of Scoring and Condition" />
            <Button mnemonicParsing="false" onAction="#exit" text="Exit" />
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </VBox>
      <HBox alignment="CENTER">
         <children>
            <Button fx:id="darkSky" mnemonicParsing="false" onMouseClicked="#goToDarkSky" prefHeight="56.0" prefWidth="247.163" />
         </children></HBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
