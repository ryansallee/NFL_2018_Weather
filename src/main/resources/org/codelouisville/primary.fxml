<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="261.0" prefWidth="441.0" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.codelouisville.PrimaryController">
   <children>
      <Label text="Welcome to NFL Weather" />
      <Button fx:id="primaryButton" onAction="#switchToScatter" text="View Scatter Chart of Scoring and Temperature" />
      <Button mnemonicParsing="false" onAction="#switchToTemperatureBar" text="View Bar CHart of Scoring and Temperature" />
      <Button mnemonicParsing="false" onAction="#exit" text="Exit" />
      <HBox prefHeight="100.0" prefWidth="200.0" />
         <ImageView fitHeight="48.0" fitWidth="121.0">
            <image>
               <Image url="https://darksky.net/dev/img/attribution/poweredby.png" />
            </image>
         </ImageView>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
